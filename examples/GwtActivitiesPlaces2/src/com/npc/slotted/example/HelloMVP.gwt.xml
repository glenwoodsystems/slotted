<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="HelloMVP">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.theme.clean.Clean'/>
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.place.Place"/>
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="com.googlecode.slotted.Slotted"/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.npc.slotted.example.client.HelloMVP'/>

    <!-- Use ClientFactoryImpl by default -->
    <replace-with class="com.npc.slotted.example.client.ClientFactoryImpl">
        <when-type-is class="com.npc.slotted.example.client.ClientFactory"/>
    </replace-with>
</module>